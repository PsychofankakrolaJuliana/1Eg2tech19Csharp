//Zad.1
//Zczytywanie liczb z pliku
StreamReader sr = new StreamReader("C:\\Users\\uczen\\przyklad.txt");
int[] T = new int[500];
for (int i = 0; i < 500; i++)
{
    T[i] = int.Parse(sr.ReadLine());
}
for(int i = 0; i < 500; i++)
{
    Console.Write(T[i] + "\t");
}
